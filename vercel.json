{
    "functions": {
      "api/**/*": {
        "runtime": "@vercel/node"
      }
    },
    "rewrites": [
      { "source": "/fr", "destination": "/index_fr.html" },
      { "source": "/fr/(.*)", "destination": "/index_fr.html" },
      { "source": "/:path*", "destination": "/index.html" }
    ]
}
  