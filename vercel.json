{
    "builds": [
      { "src": "app.js", "use": "@vercel/static-build" },
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/app.js", "dest": "/app.js" },
      { "src": "/server.js", "dest": "/server.js" },
      { "src": "/fr", "dest": "/index_fr.html" },
      { "src": "/fr/(.*)", "dest": "/index_fr.html" },
      { "src": "/:path*", "dest": "/index.html" }
    ]
}
  