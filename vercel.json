{
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/fr", "dest": "/public/index_fr.html" },
      { "src": "/fr/(.*)", "dest": "/public/index_fr.html" },
      { "src": "/:path*", "dest": "/public/index.html" },
      { "src": "/(.*)", "dest": "/index.js" }
    ]
  }
  